{
  "name": "hdb-resale",
  "version": "2.0.0",
  "description": "My Web Visualization Project (rewritten in React)",
  "main": "server_dist/server.js",
  "scripts": {
    "serve": "babel-watch server_src/server.dev.js",
    "build": "webpack --progress --colors --config ./webpack.prod.config.js",
    "deploy": "npm run build && AWS_PROFILE=yongjun21 aws s3 cp dist s3://hdbresale.yongjun.sg/ --recursive",
    "deploy:functions": "deploy-functions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yongjun21/hdb-resale.git"
  },
  "author": "Yong Jun",
  "contributors": [
    "Albert Salim",
    "Thong Yong Jun",
    "Yap Hong Kheng"
  ],
  "bugs": {
    "url": "https://github.com/yongjun21/hdb-resale/issues"
  },
  "homepage": "https://github.com/yongjun21/hdb-resale#readme",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "lodash": "^4.17.21",
    "pouchdb": "^6.4.3",
    "query-string": "^4.3.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router-dom": "^4.3.1",
    "sg-heatmap": "^3.4.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@st-graphics/backend": "^0.1.71",
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-watch": "^2.0.3",
    "css-loader": "^0.25.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^3.1.11"
  },
  "standard": {
    "global": [
      "Plotly",
      "google"
    ]
  }
}
